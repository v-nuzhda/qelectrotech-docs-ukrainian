<titleblocktemplate name="ДСТУ EN ISO 7200 A4">
    <information></information>
    <logos/>
    <grid cols="133px;125px;r100%;65px;84px;37px;37px;" rows="15;15;15;15;15;15;">
        <field align="left" hadjust="true" name="" row="0" displaylabel="false" fontsize="7" valign="center" col="0">
            <value>
                <translation lang="uk">Відповідальна організація</translation>
            </value>
            <label/>
        </field>
        <field align="left" hadjust="true" name="" row="0" displaylabel="false" fontsize="7" valign="center" col="1">
            <value>
                <translation lang="uk">Технічне узгодження</translation>
            </value>
            <label/>
        </field>
        <field align="left" colspan="1" hadjust="true" name="" row="0" displaylabel="false" fontsize="7" valign="center" col="2">
            <value>
                <translation lang="uk">Вид документа</translation>
            </value>
            <label/>
        </field>
        <field align="center" colspan="2" hadjust="true" name="" row="0" displaylabel="true" fontsize="7" valign="center" col="4">
            <value>
                <translation lang="uk">Статус документа</translation>
