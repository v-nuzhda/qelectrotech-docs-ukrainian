        <field align="center" hadjust="true" name="" row="4" displaylabel="false" fontsize="7" valign="center" col="5">
            <value>
                <translation lang="uk">Мова</translation>
            </value>
            <label/>
        </field>
        <field align="center" hadjust="true" name="" row="4" displaylabel="true" fontsize="7" valign="center" col="6">
            <value>
                <translation lang="uk">Аркуш</translation>
            </value>
            <label/>
        </field>
        <field align="left" hadjust="true" name="" row="5" displaylabel="false" fontsize="10" valign="center" col="1">
            <value>
                <translation lang="en">%approved-by</translation>
            </value>
            <label/>
        </field>
        <field align="center" hadjust="true" name="Revision" row="5" displaylabel="false" fontsize="10" valign="center" col="3">
            <value>
                <translation lang="en">%indexrev</translation>
            </value>
            <label/>
