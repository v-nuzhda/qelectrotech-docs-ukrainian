            <value>
                <translation lang="fr">%{folio-id}/%{folio-total}</translation>
                <translation lang="nl">%{folio-id}/%{folio-total}</translation>
                <translation lang="en">%{folio-id}/%{folio-total}</translation>
            </value>
            <label/>
