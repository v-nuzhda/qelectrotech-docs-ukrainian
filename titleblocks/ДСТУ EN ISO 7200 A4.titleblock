                <translation lang="uk">Технічне узгодження</translation>
                <translation lang="en">Ref</translation>
            </label>
        </field>
        <field align="left" colspan="1" hadjust="true" name="author" row="1" displaylabel="false" fontsize="10" valign="center" col="2">
            <value>
                <translation lang="en">%projecttitle</translation>
            </value>
            <label>
                <translation lang="fr">auteur</translation>
                <translation lang="pl">Plik</translation>
                <translation lang="pt">Ficheiro</translation>
                <translation lang="ro">Fisier</translation>
                <translation lang="el">Αρχείο</translation>
                <translation lang="ru">Файл</translation>
                <translation lang="da">Forfatter</translation>
                <translation lang="hr">Datoteka</translation>
                <translation lang="ca">Fitxer</translation>
                <translation lang="it">File</translation>
                <translation lang="nl">auteur</translation>
                <translation lang="cs">Soubor</translation>
                <translation lang="es">Archivo</translation>
                <translation lang="en">author</translation>
                <translation lang="ar">ملف</translation>
                <translation lang="de">Autor</translation>
            </label>
        </field>
        <field align="center" colspan="2" hadjust="true" name="" row="1" displaylabel="false" fontsize="9" valign="center" col="4">
            <value>
                <translation lang="en">%project-status</translation>
            </value>
            <label/>
        </field>
        <field align="center" hadjust="true" name="" row="2" displaylabel="false" fontsize="10" valign="center" col="0" rowspan="3">
            <value>
                <translation lang="uk">Власник документа</translation>
